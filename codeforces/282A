#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main(){
    int c;
    cin >> c;
    string x;
    int count = 0;
    while (c>0){
        c--;
        cin >> x;
        if (x[1] == '+'){
            count++;
        } else{
            count--;
        }
    }
    cout << count;
    return 0;
}
